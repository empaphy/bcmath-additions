includes:
    - phar://phpstan.phar/conf/bleedingEdge.neon
    - phpstan-baseline.neon

parameters:
    level: 8
    paths:
        - src/
        - tests/
    resultCachePath: var/cache/phpstan/resultCache.php
    treatPhpDocTypesAsCertain: false

services:
    cacheStorage:
        arguments:
            directory: var/cache/phpstan/cache/PHPStan
        alteration: true
